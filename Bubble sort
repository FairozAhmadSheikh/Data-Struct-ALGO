let arr = [99,88,75, 354,102, 225, 105];
function recursiveBubbleSort(array, length) {
  if (length == 1) {
    return;
  }
  for (let i = 0; i < length - 1; i++) {
    if (array[i] > array[i + 1]) {
      let temp = array[i];
      array[i] = array[i + 1];
      array[i + 1] = temp;
    }
  }
  recursiveBubbleSort(array,length-1)
}

recursiveBubbleSort(arr, arr.length);
console.log(arr)
